{
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "files.insertFinalNewline": true,
    "python.defaultInterpreterPath": "./venv/bin/python",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "cSpell.userWords": [
        "yakimaherald"
    ],
    "editor.suggestSelection": "first",
    "html.suggest.html5": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.tokenColorCustomizations": {
        "[Material Theme High Contrast]": {
            "textMateRules": [
                {
                    "name": "",
                    "scope": "keyword.control.import.python",
                    "settings": {
                        "foreground": "#68bbf3",
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "",
                    "scope": "keyword.control",
                    "settings": {
                        "foreground": "#f36868",
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Import Name",
                    "scope": "entity.other.inherited-class.python",
                    "settings": {
                        "foreground": "#f7ff5c"
                    }
                },
                {
                    "name": "Import Name",
                    "scope": "entity.name.namespace",
                    "settings": {
                        "foreground": "#f7ff5c"
                    }
                },
                {
                    "name": "Class Name",
                    "scope": "entity.name.type.class",
                    "settings": {
                        "fontStyle": "underline",
                        "foreground": "#7ab583"
                    }
                },
                {
                    "name": "Decorator definition decorator",
                    "scope": "punctuation.definition.decorator.python",
                    "settings": {
                        "foreground": "#F92672",
                        "underline": true,
                    }
                },
                {
                    "name": "Meta function-call",
                    "scope": "meta.function-call.generic.python",
                    "settings": {
                        "foreground": "#66D9EF"
                    }
                },
                {
                    "name": "Storage type function async",
                    "scope": "storage.type.function.async.python",
                    "settings": {
                        "foreground": "#F92672",
                    }
                },
                {
                    "name": "Punctuation separator period",
                    "scope": "punctuation.separator.period.python",
                    "settings": {
                        "foreground": "#F8F8F2",
                    }
                },
                {
                    "name": "Entity name function decorator",
                    "scope": "entity.name.function.decorator.python",
                    "settings": {
                        "foreground": "#66D9EF",
                    }
                },
                {
                    "name": "Entity name type class",
                    "scope": "entity.name.type.class.python",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "name": "Entity other inherited-class",
                    "scope": "entity.other.inherited-class.python",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Support type python",
                    "scope": "support.type.python",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "name": "String quoted docstring multi python",
                    "scope": "string.quoted.docstring.multi.python",
                    "settings": {
                        "foreground": "#88846F",
                    }
                }
            ]
        }
    },
    "html.format.indentInnerHtml": true,
    "workbench.editor.showTabs": "single",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.py$",
                "cmd": "black \"${file}\""
            },
            {
                "match": "\\.py$",
                "cmd": "autoflake --in-place --remove-all-unused-imports \"${file}\""
            }
        ]
    },
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "",
    "terminal.external.osxExec": "",
    "terminal.external.windowsExec": "",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.altClickMovesCursor": false,
    "html.format.indentHandlebars": true,
    "files.associations": {
        "**/*.html": "html",
        "**/templates/**/*.html": "jinja-html",
        "**/templates/**/*": "jinja-html",
        "**/*.jinja": "jinja-html",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements",
        "*.jinja": "html"
    },
    "emmet.includeLanguages": {
        "django-html": "html",
        "javascript": "javascriptreact"
    },
    "beautify.language": {
        "html": [
            "htm",
            "html",
            "django-html"
        ]
    },
    "[django-html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "python.analysis.diagnosticSeverityOverrides": {
        "reportOptionalMemberAccess": "none",
        "reportOptionalSubscript": "none",
    },
    "python.formatting.provider": "black",
    "cSpell.enableFiletypes": [
        "django-html"
    ],
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreview": false,
    "workbench.colorTheme": "Material Theme High Contrast",
    "workbench.startupEditor": "none",
    "git.openRepositoryInParentFolders": "always",
    "[python]": {
        "editor.formatOnType": true
    },
    "window.zoomLevel": 1,
    "workbench.editor.empty.hint": "hidden",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<tab>"
            ],
            "after": [
                "<esc>",
                ">",
                ">"
            ]
        },
        {
            "before": [
                "r"
            ],
            "after": [
                "<C-r>"
            ]
        },
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "d", "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ]
}
