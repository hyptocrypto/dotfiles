{
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "files.insertFinalNewline": true,
    "workbench.editor.untitled.hint": "hidden",
    "python.defaultInterpreterPath": "./venv/bin/python",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "cSpell.userWords": [
        "yakimaherald"
    ],
    "workbench.colorTheme": "Material Theme High Contrast",
    "editor.suggestSelection": "first",
    "html.suggest.html5": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.tokenColorCustomizations": {
        "[Material Theme High Contrast]": {
            "textMateRules": [
                {
                    "name": "Keywords",
                    "scope": "keyword.control.flow.python",
                    "settings":{
                        "foreground": "#f36868", "fontStyle": "italic"
                    }
                },
                {
                    "name": "Import Name",
                    "scope": "entity.other.inherited-class.python",
                    "settings": {
                        "foreground": "#f7ff5c"
                    }
                },
                {
                    "name": "Import Name",
                    "scope": "entity.name.namespace",
                    "settings": {
                        "foreground": "#f7ff5c"
                    }
                },
                {
                    "name": "Class Name",
                    "scope": "entity.name.type.class",
                    "settings": {
                        "fontStyle": "underline",
                        "foreground": "#7ab583"
                    }
                },
                {
                    "name": "Decorator definition decorator",
                    "scope": "punctuation.definition.decorator.python",
                    "settings": {
                        "foreground": "#F92672",
                        "underline": true,
                    }
                },
                {
                    "name": "Meta function-call",
                    "scope": "meta.function-call.generic.python",
                    "settings": {
                        "foreground": "#66D9EF"
                    }
                },
                {
                    "name": "Storage type function async",
                    "scope": "storage.type.function.async.python",
                    "settings": {
                        "foreground": "#F92672",
                    }
                },
                {
                    "name": "Punctuation separator period",
                    "scope": "punctuation.separator.period.python",
                    "settings": {
                        "foreground": "#F8F8F2",
                    }
                },
                {
                    "name": "Entity name function decorator",
                    "scope": "entity.name.function.decorator.python",
                    "settings": {
                        "foreground": "#66D9EF",
                    }
                },
                {
                    "name": "Entity name type class",
                    "scope": "entity.name.type.class.python",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "name": "Entity other inherited-class",
                    "scope": "entity.other.inherited-class.python",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Support type python",
                    "scope": "support.type.python",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "name": "String quoted docstring multi python",
                    "scope": "string.quoted.docstring.multi.python",
                    "settings": {
                        "foreground": "#88846F",
                    }
                }
            ]
        }
    },
    "html.format.indentInnerHtml": true,
    "workbench.editor.showTabs": false,
    "workbench.editor.enablePreview": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.py$",
                "cmd": "black \"${file}\""
            },
            {
                "match": "\\.py$",
                "cmd": "autoflake --in-place --remove-all-unused-imports \"${file}\""
            }
        ]
    },
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "",
    "terminal.external.osxExec": "",
    "terminal.external.windowsExec": "",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.altClickMovesCursor": false,
    "html.format.indentHandlebars": true,
"files.associations": {
    "**/*.html": "html",
    "**/templates/**/*.html": "django-html",
    "**/templates/**/*": "django-txt",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements"
},
"emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact"
},
"beautify.language": {
    "html": [
        "htm",
        "html",
        "django-html"
    ]
},
"[django-html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
},
"python.analysis.diagnosticSeverityOverrides": {
    "reportOptionalMemberAccess": "none",
    "reportOptionalSubscript": "none",
},
"python.formatting.provider": "black",
"cSpell.enableFiletypes": [
    "django-html"
]
}

